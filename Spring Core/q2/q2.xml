<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id="questionList" class="q2.Question">
		<property name="questionId" value="1"/>
		<property name="question" value="Your Family Details"/>
		<property name="answersList">
			<list>
				<value>Vinay</value>
				<value>Ravi</value>
				<value>Ramesh</value>
				<value>Virat</value>
			</list>
		</property>
	
		<property name="answersSet">
			<set>
			<value>Vinay</value>
				<value>Ravi</value>
				<value>Ramesh</value>
				<value>Virat</value>
			</set>
		</property>
		<property name="answersMap">
			<map>
				 <entry key="1" value="Vinay"/>
	            <entry key="2" value="Ravi"/>
	            
	            <entry key="3" value="Ramesh"/>
	            <entry key="4" value="Virat"/>
